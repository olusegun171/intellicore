{
    "name": "intellicore/pin-generator",
    "description": "A laravel package to generate and emit PINs suitable for use with cash cards, door locks etc",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Intellicore\\PinGenerator\\": "src/"

        }
    },
    "authors": [
        {
            "name": "Olusegun Mustapha",
            "email": "olusegun171@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Intellicore\\PinGenerator\\GeneratorServiceProvider"
            ],
            "aliases": {
                "IntellicoreGenerate": "Intellicore\\PinGenerator\\Facades\\IntellicoreGenerate"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/support": "~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}